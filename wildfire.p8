init_state={
	init=function()
		poke(0x5f2c,3) -- 64x64 mode
		cls()
		cdata.init("notcalle_wildfire",1)
		game.next(menu_state)
	end
}

--
-- cartridge data
--
cdata={
	magic_number=0xdeadbeef,
	magic=0,
	version=2,
	last_time=2,
	best_time=3,

	opt={
		burnout=59,
		healing=60,
		max_bkt=61,
		difficulty=62,
		music=63,
	},

	init=function(name,version)
		cartdata(name)
		if(dget(cdata.magic)==cdata.magic_number and dget(cdata.version)==version) return
		dset(cdata.magic,cdata.magic_number)
		dset(cdata.version,version)
		for n=2,63 do
			dset(n,0)
		end
		dset(cdata.opt.max_bkt,opt.max("max_bkt"))
	end,
}

-- sprite flag definitions
fl_b={
	fire=0,
	block=1,
	item=2,
	wet=3,
	water=4,
	rescue=6,
	last=7
	}
fl_m={
	fire=0x01,
	block=0x02,
	item=0x04,
	wet=0x08,
	water=0x10,
	rescue=0x40,
	last=0x80
	}

function fl_tst(mask,flags)
	return band(mask,flags)~=0
end

ctext={
	init=function(my,str)
		my.str=str
	end,

	draw=function(my,x,y,c)
		local xo=-2*#my.str
		print(my.str,x+xo,y,c)
	end,
}

firetext={
	draw=function(str)
		local txt={}
		ctext.init(txt,"‚àß "..str.." ‚àß  ")
		camera() clip()
		rectfill(0,28,64,36,8)
		for n=1,4 do
			ctext.draw(txt,31+rnd(3),29+rnd(3),9+n%2)
		end
		ctext.draw(txt,32,30,8)
	end,
}

function cprint(str,y,c)
	local x=32-2*#str

	print(str,x,y,c)
end

--
-- game menu screen
--

menu_state={
	init=function()
		mus.play(0)
	end,

	update=function()
		if padp.bo then
			game.next(options_state)
		elseif padp.bx then
			game.next(world_state)
		end
	end,

	draw=function()
		if(not game.nth_tick(2)) return
		cls(0)
		firetext.draw("wildfire")
		cprint("‚ùé to start ",45,8)
		cprint("üÖæÔ∏è for options ",52,8)
	end,
}

--
-- game options screen
--

opt={
	music={
		l={"music","no music"},on=0,
	},
	difficulty={
		l={"normal","nightmare","custom"},
		update=function(v)
			if v==0 then
				opt._set("max_bkt",opt.max("max_bkt"))
				opt._set("healing",opt["healing"].on)
				opt._set("burnout",opt["burnout"].on)
			elseif v==1 then
				opt._set("max_bkt",opt.min("max_bkt"))
				opt._set("healing",1-opt["healing"].on)
				opt._set("burnout",1-opt["burnout"].on)
			end
		end,
	},
	max_bkt={
		min=1,
		max=5,
		str=function(v)
			return tostr(v).." buckets"
		end,
		update=function()
			opt._set("difficulty",opt.max("difficulty"))
		end,
	},
	healing={
		l={"water heals","no healing"},on=0,
		update=function()
			opt._set("difficulty",opt.max("difficulty"))
		end,
	},
	burnout={
		l={"burns out","burn 4 ever"},on=0,
		update=function()
			opt._set("difficulty",opt.max("difficulty"))
		end,
	},

	str=function(o)
		local v=opt.get(o)
		if(opt[o].str) return opt[o].str(v)
		return opt[o].l[v+1]
	end,

	min=function(o)
		if(opt[o].min) return opt[o].min
		return 0
	end,

	max=function(o)
		if(opt[o].max) return opt[o].max
		return #opt[o].l-1
	end,

	get=function(o)
		return mid(opt.min(o),dget(cdata.opt[o]),opt.max(o))
	end,

	_set=function(o,v)
		dset(cdata.opt[o],mid(opt.min(o),v,opt.max(o)))
	end,

	set=function(o,v)
		opt._set(o,v)
		if(opt[o].update) opt[o].update(v)
	end,

	on=function(o)
		return opt[o].on==opt.get(o)
	end,
}

options_state={
	opt={
		"music",
		"difficulty",
		"max_bkt",
		"healing",
		"burnout",
	},

	init=function(my)
		mus.stop()
		my.sel=1
	end,

	update=function(my)
		local sel=my.sel+padp.dy
		local dv=padp.dx

		my.sel=mid(1,sel,#my.opt)

		if dv~=0 then
			local k=my.opt[my.sel]
			opt.set(k,opt.get(k)+dv)
		end

		if(padp.bo or padp.bx) game.next(menu_state)
	end,

	draw=function(my)
		cls(0)

		for n=1,#my.opt do
			local k=my.opt[n]
			local str=opt.str(k)
			local v=opt.get(k)

			clip() camera(-1,6)
			if n==my.sel then
				local c
				c=8 if(v==0) c=5
				print("‚¨ÖÔ∏è",0,n*7,c)
				c=8 if(v==opt.max(k)) c=5
				print("‚û°Ô∏è",55,n*7,c)
				cprint(str,n*7,8)
			else
				cprint(str,n*7,5)
			end
			n+=1
		end
	end,
}

