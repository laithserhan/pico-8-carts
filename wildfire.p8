__lua__
-- wildfire
-- by notcalle  #lowrezjam 2018

tick=0
thunder_tick=tick
banner_tick=tick
max_ht=100.0
game_state={}

-- sprite flag definitions
fl_b={
	fire=0,
	block=1,
	item=2,
	grow=3,
	water=4
	}
fl_m={
	fire=0x01,
	block=0x02,
	item=0x04,
	grow=0x08,
	water=0x10
	}

function fl_tst(mask,flags)
	return band(mask,flags)~=0
end

--
-- game menu screen
--

function update_menu()
	if btnp(4) or btnp(5) then
		next_state=world_state
	end
end

function draw_menu()
	if(tick%2~=0) return
	cls(9)
	rectfill(0,28,64,36,8)
	print("‚àß wildfire ‚àß",4+rnd(3),29+rnd(3),10)
	print("‚àß wildfire ‚àß",4+rnd(3),29+rnd(3),9)
	print("‚àß wildfire ‚àß",4+rnd(3),29+rnd(3),10)
	print("‚àß wildfire ‚àß",4+rnd(3),29+rnd(3),9)
	print("‚àß wildfire ‚àß",5,30,8)

	print("‚ùé/üÖæÔ∏è",22,52,8)
	print("to start",16,58,8)
end

menu_state={
	draw=draw_menu,
	update=update_menu
	}

--
-- game over screen
--

function update_gameover()
	if btnp(4) or btnp(5) then
		extcmd("reset")
	end
end

function draw_gameover()
	rectfill(0,28,64,36,8)
	print("‚àß game over ‚àß",1+rnd(3),29+rnd(3),10)
	print("‚àß game over ‚àß",1+rnd(3),29+rnd(3),9)
	print("‚àß game over ‚àß",1+rnd(3),29+rnd(3),10)
	print("‚àß game over ‚àß",1+rnd(3),29+rnd(3),9)
	print("‚àß game over ‚àß",2,30,8)
end

gameover_state={
	draw=draw_gameover,
	update=update_gameover
	}

--
-- running game
--

banner={
	tick=0,
	str=nil,
	}

function draw_banner()
	if(banner.tick<=tick) return

	local str=banner.str
	
	clip(0,39,64,46)
	camera(banner.x,-40)
	print(str,rnd(3),rnd(3),10)
	print(str,rnd(3),rnd(3),9)
	print(str,rnd(3),rnd(3),10)
	print(str,rnd(3),rnd(3),9)
	print(str,1,1,0)
	banner.x+=1
end

function set_banner(str)
	local ticks=64+4*#str
	banner.x=-64
	banner.str=str
	banner.tick=tick+ticks
end

-- items

item={
	bucket=64,
	bucket_full=65
	}

function iset(x,y,item)
	mset(64+x,y,item)
end

function iget(x,y)
	return mget(64+x,y)
end

function take_item(x,y)
	local ispr=iget(x,y)
	if ispr==item.bucket or ispr==item.bucket_full then
		play.n_bkt+=1
		if(ispr==item.bucket_full) play.bkt+=1
		set_banner("bucket taken",0)
	end
	iset(x,y,0)
end

-- player data

play={
	x=256,
	y=256,
	dx=0,
	dy=0,
	hd_x=1,
	hd_y=0,
	ht=max_ht,
	bkt=0,
	n_bkt=0
	}

function move_player()
	local mov_x=0
	local mov_y=0

	if(btn(0)) mov_x-=1
	if(btn(1)) mov_x+=1
	if(btn(2)) mov_y-=1
	if(btn(3)) mov_y+=1
	if mov_x~=0 or mov_y~=0 then
		play.hd_x=mov_x
		play.hd_y=mov_y
	end
	play.dx=mov_x
	play.dy=mov_y
	play.x=mid(0,play.x+mov_x,504)
	play.y=mid(0,play.y+mov_y,504)
end

function collide_player()
	local flags=0
	for d_x=0,7,7 do
		local map_x=(play.x+d_x)/8
		for d_y=0,7,7 do
			local map_y=(play.y+d_y)/8
			local	flags=fget(iget(map_x,map_y))
			if fl_tst(fl_m.fire,flags) then
				play.ht-=1
			else
				play.ht=min(play.ht*1.0001,max_ht)
			end
			if fl_tst(fl_m.water,flags) then
				if(play.bkt) play.bkt=1
			end
			if fl_tst(fl_m.block,flags) then
				play.x-=play.dx
				play.y-=play.dy
			end
			if fl_tst(fl_m.item,flags) then
				take_item(map_x,map_y)
			end
		end
	end
end

function use_bucket()
	if(play.bkt<1) return

	for x=0,1 do
		local mx=(play.x+7*x)/8+play.hd_x
		for y=0,1 do
			local my=(play.y+7*y)/8+play.hd_y
			iset(mx,my,12)
		end
	end
	play.ht=min(play.ht*1.25,max_ht)
	play.bkt-=1
end

function update_player()
	move_player()
	collide_player()
	if(btnp(4)) use_bucket()
	if play.ht<=0 then
		next_state=gameover_state
	end
end

function set_fire(x,y,always)
	local mm_x=112+x/4
	local mm_y=48+y/4
 local m_spr=mget(x,y)
	if(fget(iget(x,y),fl_b.water)) return
	if fget(iget(x,y),fl_b.grow) then
		iset(x,y,0)
		return
	end 
	if fget(m_spr,fl_b.fire) or always then
		sset(mm_x,mm_y,8)
		iset(x,y,15)
	end
 if(fget(m_spr,fl_b.block)) return
	mset(x,y,mget(x,y)+1)
end

function on_fire(x,y)
	if x<0 or y<0 or x>63 or y>64 then
		return false
	end
	return fget(iget(x,y),fl_b.fire)
end

function update_fire(x,y)
	if(on_fire(x,y)) return

	local prob=0
	for d_x=-1,1 do
		for d_y=-1,1 do
		 local ispr=iget(x+d_x,y+d_y)
			if fget(ispr,fl_b.fire) then
				prob+=0.1
			elseif fget(ispr,fl_b.water) then
				prob-=0.05
			end
		end
	end
	if rnd(1)<prob then
		set_fire(x,y)
	end
end

function animate_fire()
	local x=flr(rnd(8))
	local y=flr(rnd(8))
	local c=(flr(rnd(3))+8)%11
	sset(x+120,y,c)
end

function update_thunder(prob)
	if rnd(1) < prob then
		local thun_x=flr(rnd(64))
		local thun_y=flr(rnd(64))

		set_fire(thun_x,thun_y,true)

		thunder_tick=tick+flr(4+rnd(4))
		sfx(0)
	end
end

function thunderstrike()
	return thunder_tick>tick
end

function update_world()
	update_player()

	world_x=mid(0,play.x-28,448)
	world_y=mid(0,play.y-20,464)

	update_thunder(0.01)
	for x=0,63 do
		update_fire(x,tick%64)
	end

	animate_fire()
end

function draw_map()
	local cam_x=world_x%8
	local cam_y=world_y%8
	local map_x=flr(world_x/8)
	local map_y=flr(world_y/8)

	clip(0,0,64,48)
	camera(cam_x,cam_y)
	map(map_x,map_y,0,0,9,7)
	map(map_x+64,map_y,0,0,9,7)
end

function draw_minimap()
	clip(48,48,16,16)
	camera(-48,-48)
	rectfill(0,0,16,16,0)
	spr(110,0,0,2,2)
	if tick%2 == 1 then
		pset(play.x/32,play.y/32,15)
	end
end

function draw_player()
	local pspr=33+play.hd_x+16*play.hd_y

	clip(0,0,64,48)
	camera(world_x,world_y)
	spr(pspr,play.x,play.y)
end

function draw_ui()
	camera(0,-48)
	clip(0,48,48,16)
	rectfill(0,0,64,16,9)

	ht_px=flr(46*play.ht/max_ht)
	rectfill(1,1,46,5,11)
	if ht_px<46 then
		rectfill(ht_px,1,46,5,8)
	end
	print("health",12,1,0)

	if play.n_bkt>0 then
		for n=1,play.bkt do
			spr(item.bucket_full,n*4-3,7)
		end
		for n=play.bkt+1,play.n_bkt do
			spr(item.bucket,n*4-3,7)
		end
	end

	clip()
	camera()
	local ts=flr(tick/30)
	local tm=flr(ts/60)
	local tstr
	ts=ts%60
	if ts<10 then
		tstr=tm..":0"..ts
	else
		tstr=tm..":"..ts
	end
	print(tstr,64-4*#tstr,1,8)
end

function draw_world()
	cls(6+flr(rnd(2)))
	if not thunderstrike() then
		draw_map()
	end

	draw_player()
	draw_minimap()
	draw_ui()
	draw_banner()
end

world_state={
	draw=draw_world,
	update=update_world
	}

function init_world()
	local r1,r2,rt
	for n=0,63 do
		r1=rnd(2) rt=rnd(4) r2=max(r1,rt) r1=min(r1,rt)
		for c=0,r2 do
			if c<r1 then
				iset(n,c,13)
			else
				iset(n,c,14)
			end
		end

		r1=rnd(2) rt=rnd(4) r2=max(r1,rt) r1=min(r1,rt)
		for c=0,r2 do
			if c<r1 then
				iset(c,n,13)
			else
				iset(c,n,14)
			end
		end

		r1=rnd(2) rt=rnd(4) r2=max(r1,rt) r1=min(r1,rt)
		for c=0,r2 do
			if c<r1 then
				iset(63-n,63-c,13)
			else
				iset(63-n,63-c,14)
			end
		end

		r1=rnd(2) rt=rnd(4) r2=max(r1,rt) r1=min(r1,rt)
		for c=0,r2 do
			if c<r1 then
				iset(63-c,63-n,13)
			else
				iset(63-c,63-n,14)
			end
		end
	end
end

--
-- main loop
--

function _init()
	poke(0x5f2c,3) -- 64x64 mode
	init_world()
	next_state=menu_state
end

function _update()
	tick+=1
	if game_state.update then
		game_state.update()
	end
end

function _draw()
	if game_state.draw then
		camera()
		clip()
		game_state.draw()
	end
	game_state=next_state
end

