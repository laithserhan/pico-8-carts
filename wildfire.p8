--
-- running game
--

banner={
	init=function(my,str)
		my.tick=0
		my.str=str
		my.x=0
		my.next_state=nil
	end,

	update=function(my)
		if my.tick<game.tick() then
			my.str=nil
			if(my.next_state) game.next(my.next_state)
			return
		end
		my.x+=1
	end,

	draw=function(my)
		if(not my.str) return

		clip(0,39,64,46)
		camera(my.x,-40)
		for n=1,4 do
			print(my.str,rnd(3),rnd(3),9+n%2)
		end
		print(my.str,1,1,0)
	end,

	new=function(str,next_state)
		my=banner -- fixme
		if(my.str == str) return
		my.x=-64
		my.str=str
		my.tick=game.tick()+64+4*#str
		my.next_state=next_state
	end,
}

mus={
	play=function(pat,delay,mask)
		if(not opt.on("music")) return
		music(pat,delay,mask)
	end,

	stop=function()
		music(-1)
	end,
}

-- items

item={
	bucket=64,
	bucket_full=65,

	take=function(play,x,y)
		local ispr=iget(x,y)
		if ispr==item.bucket or ispr==item.bucket_full then
			if play.n_bkt>= opt.get("max_bkt") then
				banner.new("can't carry more buckets")
				return
			end
			play.n_bkt+=1
			if(ispr==item.bucket_full) play.bkt+=1
			banner.new("bucket taken")
		end
		iset(x,y,0)
	end,
}

function iset(x,y,item)
	mset(64+x,y,item)
end

function iget(x,y)
	return mget(64+x,y)
end

-- player data

player={
	max_ht=100.0,

	init=function(my)
		my.x=256-24
		my.y=256-8
		my.dx=0
		my.dy=0
		my.hd_x=1
		my.hd_y=0
		my.ht=my.max_ht
		my.en=my.max_ht
		my.bkt=0
		my.n_bkt=0
		my.died=false
		my.on_pad=false
	end,

	hurt=function(my,dmg)
		if(konami.on) return
		my.ht=max(0,my.ht-dmg)
		my.en=min(my.en,my.ht)
		if(my.ht==0) my.died=true
	end,

	tire=function(my,pow)
		if(konami.on) return
		local en=my.en-max(pow-1,0)
		my.en=max(0,en)
		if en<0 then
			my.hurt(my,en/-20)
		end
	end,

	heal=function(my,rate)
		if(not opt.on("healing")) return
		local ht=(1+my.ht)*(1+rate)
		my.ht=min(ht,my.max_ht)
	end,

	rest=function(my,rate)
		local en=(1+my.en)*(1+rate)
		my.en=min(en,my.ht)
	end,

	move=function(my,spd)
		local mov_x=pad.dx
		local mov_y=pad.dy

		if mov_x~=0 or mov_y~=0 then
			my.hd_x=mov_x
			my.hd_y=mov_y
			my.tire(my,spd)
			if(my.en<my.max_ht/4) spd/=2
		else
			my.rest(my,0.01)
		end

		local dx=mov_x*spd
		local dy=mov_y*spd

		my.x=mid(0,my.x+dx,504)
		my.y=mid(0,my.y+dy,504)
		my.collide(my,dx,dy)
	end,

	collide=function(my,dx,dy)
		local mflags=0
		local iflags=0

		for d_x=0,7,7 do
			local map_x=(my.x+d_x)/8
			for d_y=0,7,7 do
				local map_y=(my.y+d_y)/8
				local iflag=fget(iget(map_x,map_y))
				local mflag=fget(mget(map_x,map_y))
				iflags=bor(iflags,iflag)
				mflags=bor(mflags,mflag)

				if fl_tst(fl_m.fire,iflag) then
					my.hurt(my,1)
				end

				if fl_tst(fl_m.item,iflag) then
					item.take(my,map_x,map_y)
				end

				my.on_pad=fl_tst(fl_m.rescue,mflag)

				if fl_tst(fl_m.rescue,band(iflag,mflag)) and rescue.landed then
					game.next(rescued_state)
				end
			end
		end

		local aflags=bor(mflags,iflags)

		if fl_tst(fl_m.water,mflags) then
			my.bkt=min(my.n_bkt,my.bkt+1)
			my.heal(my,0.001)
		end

		if fl_tst(fl_m.block,aflags) then
			my.x-=dx
			my.y-=dy
		end
	end,

	update=function(my)
		if(my.died) return
		local pow=1
		if(pad.bx) pow*=2
		my.move(my,pow)
		if(padp.bo) my.use_bucket(my,pow)
		if(my.died) banner.new("you die",gameover_state)
	end,

	draw=function(my)
		local pspr=33+my.hd_x+16*my.hd_y

		clip(0,0,64,48)
		camera(world.x,world.y)
		spr(pspr,my.x,my.y)
	end,

	use_bucket=function(my,pow)
		if(my.bkt<1) return
		my.tire(my,pow*20)
		if(my.en<=25) pow/=2

		for x=-1,8,3 do
			local mx=(my.x+x)/8+my.hd_x*(pow+1)
			for y=-1,8,3 do
				local my=(my.y+y)/8+my.hd_y*(pow+1)
				if iget(mx,my)==0 then
					iset(mx,my,12)
				end
				if fget(iget(mx,my),fl_b.fire) then
					fire.clr(mx,my,12)
					sfx(1)
				end
			end
		end
		if(not konami.on) my.bkt-=1
	end,
}

fire={
	clr=function(x,y,sp)
		if(not fire.at(x,y)) return

		minimap.set(x,y,0)
		iset(x,y,sp)
		if opt.on("burnout") and rnd(1)<0.1 then
			mset(x,y,4)
		end
	end,

	set=function(x,y,always)
		local m_spr=mget(x,y)
		if(fget(m_spr,fl_b.water)) return
		if fget(iget(x,y),fl_b.wet) then
			iset(x,y,0)
			return
		end
		if fget(m_spr,fl_b.fire) or always then
			minimap.set(x,y,8)
			iset(x,y,15)
		end
		if(fget(m_spr,fl_b.last)) return
		mset(x,y,mget(x,y)+1)
	end,

	at=function(x,y)
		if x<0 or y<0 or x>63 or y>64 then
			return false
		end
		return fget(iget(x,y),fl_b.fire)
	end,

	animate=function()
		local x=flr(rnd(8))
		local y=flr(rnd(8))
		local c=(flr(rnd(4))+8)%11
		sset(x+120,y,c)
	end,

	update_at=function(x,y)
		local prob=0
		for d_x=-1,1 do
			for d_y=-1,1 do
				local ispr=iget(x+d_x,y+d_y)
				if fget(ispr,fl_b.fire) then
					prob+=0.11
				elseif fget(ispr,fl_b.wet) then
					prob-=0.1
				end
			end
		end
		if rnd(1)<prob then
			fire.set(x,y)
		elseif opt.on("burnout") and rnd(0.11)>prob then
			fire.clr(x,y)
		end
	end,

	update=function()
		for x=0,63 do
			fire.update_at(x,game.tick(64))
		end
	end
}

thunder={
	init=function()
		thunder.tick=0
		thunder.prob=0.005
	end,

	update=function()
		if rnd(1) < thunder.prob then
			local thun_x=flr(rnd(64))
			local thun_y=flr(rnd(64))

			fire.set(thun_x,thun_y,true)

			thunder.tick=game.tick()+flr(4+rnd(4))
			sfx(0)
		end
	end,

	strike=function()
		return thunder.tick>game.tick()
	end,
}

helicopter={
	init=function(my,x,y)
		my.x=x
		my.y=y
	end,

	animate=function(my)
		local base=66+4*flr(game.tick(4)/2)
		for dx=0,3 do
			for dy=0,3 do
				iset(my.x+dx,my.y+dy,base+dx+dy*16)
			end
		end
	end,

	explode=function(my)
		for dx=0,2 do
			fire.set(my.x+dx,my.y+1+rnd(2),true)
		end
	end,
}

rescue={
	init=function(my)
		my.can_land=false
		my.landed=false
		my.tick=flr((1+rnd(1))*60*30)
	end,

	update=function(my)
		local on_fire=false
		for x=29,34 do
			for y=29,34 do
				on_fire=on_fire or fire.at(x,y)
			end
		end

		if my.landed then
			if on_fire then
				helicopter.explode(helicopter)
				banner.new("rescue helicopter exploded",gameover_state)
			else
				helicopter.animate(helicopter)
			end
		else
			if game.tick()>my.tick then
				if on_fire then
					mus.play(1)
					banner.new("fire hazard - rescue turned away")
					my.tick+=flr((30+rnd(60))*30)
					if player.n_bkt<opt.get("max_bkt") then
						local px=player.x/8
						local py=player.y/8
						iset(px+rnd(5)-2,py+rnd(5)-2,item.bucket)
					end
				elseif player.on_pad then
					banner.new("please leave the helipad")
				else
					my.landed=true
					banner.new("helipad clear - rescue landed")
					helicopter.init(helicopter,30,30)
				end
			end
		end
	end,

	draw=function(my)
		local secs=flr(max(my.tick-game.tick(),0)/30)
		timer.draw(secs,64,8)
	end,
}

minimap={
	init=function()
		for x=0,63,4 do
			for y=0,63,4 do
				minimap.clr(x,y)
			end
		end
	end,

	draw=function()
		clip(48,48,16,16)
		camera(-48,-48)
		rectfill(0,0,16,16,0)
		spr(110,0,0,2,2)
		if game.tick(2) == 1 then
			pset(player.x/32,player.y/32,15)
		end
	end,

	clr=function(x,y)
		minimap.set(x,y,0)
	end,

	set=function(x,y,c)
		local mm_x=112+x/4
		local mm_y=48+y/4
		sset(mm_x,mm_y,c)
	end
}

timer={
	draw=function(secs,x,c)
		local m=flr(secs/60)
		local s=secs%60
		local tstr

		if s<10 then
			tstr=m..":0"..s
		else
			tstr=m..":"..s
		end

		clip()
		camera()
		print(tstr,x-4*#tstr,1,c)
	end
}

ui={
	init=function()
		init(minimap)
	end,

	draw=function()
		camera(0,-48) clip(0,48,48,16)
		rectfill(0,0,64,16,9)

		ht_px=flr(46*player.ht/player.max_ht)
		en_px=flr(46*player.en/player.max_ht)
		rectfill(1,1,46,5,8)
		if(ht_px>0)rectfill(1,1,ht_px,5,10)
		if(en_px>0)rectfill(1,1,en_px,5,11)
		print("health",12,1,0)

		if player.n_bkt>0 then
			for n=1,player.bkt do
				spr(item.bucket_full,n*7-6,7)
			end
			for n=player.bkt+1,player.n_bkt do
				spr(item.bucket,n*7-6,7)
			end
		end
		draw(minimap)
		timer.draw(world_state.time,21,0)
	end
}

water={
	line=function()
		local sum=0
		for n=1,10 do
			sum+=rnd(1)
		end
		return max(1,sum-2)
	end,

	init=function()
		for n=0,63 do
			for c=0,water.line() do
				water.set(n,c)
			end

			for c=0,water.line() do
				water.set(c,n)
			end

			for c=0,water.line() do
				water.set(63-n,63-c)
			end

			for c=0,water.line() do
				water.set(63-c,63-n)
			end
		end
	end,

	set=function(x,y)
		mset(x,y,14)
		minimap.set(x,y,12)
	end,
}

world={
	init=function(my)
		my.x=player.x
		my.y=player.y
		for ix=0,63 do
			for iy=0,63 do
				iset(ix,iy,0)
			end
		end
		init(water)
		init(thunder)
		helicopter.init(helicopter,player.x/8-1,player.y/8-4)
		helicopter.animate(helicopter)
		helicopter.explode(helicopter)

		iset(player.x/8+rnd(5)-2,player.y/8+rnd(5)-2,item.bucket)
	end,

	update=function(my)
		local dx=player.x-28-my.x
		local dy=player.y-20-my.y
		my.x=mid(0,my.x+dx/4,448)
		my.y=mid(0,my.y+dy/4,464)

		update(thunder)
		update(fire)
		fire.animate()
	end,

	draw=function(my)
		local cam_x=my.x%8
		local cam_y=my.y%8
		local map_x=flr(my.x/8)
		local map_y=flr(my.y/8)

		clip(0,0,64,48)
		camera(cam_x,cam_y)
		map(map_x,map_y,0,0,9,7)
		map(map_x+64,map_y,0,0,9,7)
	end
}

world_state={
	draw=draw_world,
	update=update_world,

	init=function()
		init(konami)
		init(player)
		init(rescue)
		init(world)
		init(ui)
		init(banner)
		banner.new("‚ùé to sprint üÖæÔ∏è to throw, rescue on its way once you clear the helipad from fire")
	end,

	update=function()
		update(konami)
		update(player)
		update(rescue)
		update(world)
		update(banner)
	end,

	draw=function(my)
		cls(6+flr(rnd(2)))
		if not thunder.strike() then
			draw(world)
		end

		draw(rescue)
		draw(player)
		draw(ui)
		draw(banner)
	end,
}

--
-- engine library
--

function noop() end
function init(my) if(my) my.init(my) end
function update(my) if(my) my.update(my) end
